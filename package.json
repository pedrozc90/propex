{
    "name": "propex",
    "version": "1.0.0",
    "description": "propex backend",
    "main": "dist/index.js",
    "scripts": {
        "build": "tsc --project tsconfig.json",
        "build:watch": "tsc --project tsconfig.json --watch",
        "clean": "rm -rf node_modules && rm -rf dist && rm package-lock.json && npm install",
        "deploy": "exit 0",
        "docker:build": "docker build --file Dockerfile --tag my_image .",
        "docker:run": "docker run --detach --publish 9000:9000 --name my_app my_image",
        "docker:stop": "docker stop my_app",
        "docker:clean": "npm run docker:stop && docker rm my_app",
        "lint": "tslint --project tsconfig.json",
        "lint:fix": "tslint --project tsconfig.json --fix",
        "start": "nodemon src/index.ts",
        "start:dev": "nodemon --watch \"src/**/*.ts\" --ignore \"node_modules/**/*\" --exec ts-node src/index.ts",
        "start:prod": "cross-env NODE_ENV=production node dist/index.js",
        "test:coverage": "cross-env NODE_ENV=test nyc mocha",
        "test:unit": "cross-env NODE_ENV=test mocha",
        "typeorm": "typeorm"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/PedroZC90/propex.git"
    },
    "keywords": [
        "api",
        "express",
        "nodejs",
        "typescript"
    ],
    "author": "PedroZC90 <pedrozc90@gmail.com>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/PedroZC90/propex/issues"
    },
    "homepage": "https://github.com/PedroZC90/propex#readme",
    "dependencies": {
        "@tsed/common": "^5.67.0",
        "@tsed/core": "^5.67.0",
        "@tsed/di": "^5.67.0",
        "@tsed/exceptions": "^5.67.0",
        "@tsed/multipartfiles": "^5.61.1",
        "@tsed/swagger": "^5.67.0",
        "@tsed/typeorm": "^5.67.0",
        "@types/swagger-schema-official": "^2.0.22",
        "body-parser": "^1.19.1",
        "compression": "^1.7.4",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "cross-env": "^7.0.2",
        "dotenv": "^8.6.0",
        "express": "^4.17.2",
        "highlight.js": "^11.4.0",
        "jsonwebtoken": "^8.5.1",
        "lodash": "^4.17.19",
        "method-override": "^3.0.0",
        "moment": "^2.27.0",
        "multer": "^1.4.4",
        "mysql2": "^2.3.3",
        "reflect-metadata": "^0.1.13",
        "ts-httpexceptions": "^4.1.0",
        "typeorm": "^0.2.41",
        "y18n": "^5.0.8"
    },
    "devDependencies": {
        "@types/body-parser": "^1.19.2",
        "@types/compression": "^1.7.2",
        "@types/cookie-parser": "^1.4.2",
        "@types/cors": "^2.8.12",
        "@types/dotenv": "^8.2.0",
        "@types/express": "^4.17.13",
        "@types/jsonwebtoken": "^8.5.8",
        "@types/method-override": "0.0.31",
        "@types/multer": "^1.4.7",
        "@types/node": "^13.13.52",
        "@typescript-eslint/eslint-plugin": "^3.7.1",
        "@typescript-eslint/parser": "^3.7.1",
        "eslint": "^7.32.0",
        "eslint-config-standard": "^14.1.0",
        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-node": "^11.0.0",
        "eslint-plugin-promise": "^4.2.1",
        "eslint-plugin-standard": "^4.0.1",
        "nodemon": "^2.0.15",
        "ts-node": "^9.1.1",
        "typescript": "^4.5.5"
    },
    "engines": {
        "node": "12.x"
    }
}
